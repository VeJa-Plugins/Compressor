@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix mod: <http://moddevices.com/ns/modgui#>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix pset: <http://lv2plug.in/ns/ext/presets#>.

<http://VeJaPlugins.com/plugins/Release/Compressor>
a lv2:Plugin, lv2:DynamicsPlugin;

doap:name "Compressor";

doap:developer [
    foaf:name "VeJa Plugins";
    foaf:homepage <>;
    foaf:mbox <mailto:jan@moddevices.com>;
    ];

doap:maintainer [
    foaf:name "VeJa Plugins";
    foaf:homepage <http://moddevices.com>;
    foaf:mbox <mailto:jan@moddevices.com>;
    ];

    lv2:minorVersion 1;
    lv2:microVersion 1;

rdfs:comment """

Veja Compressor designed for mod devices

""";

lv2:port
[
    a lv2:AudioPort, lv2:InputPort;
    lv2:index 0;
    lv2:symbol "Input";
    lv2:name "Input";
    lv2:shortname "Input";
],
[
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 1;
    lv2:symbol "Output";
    lv2:name "Output";
    lv2:shortname "Output";
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 2;
    lv2:symbol "DOWN_ON";
    lv2:name "Downwards Compression";
    lv2:shortname "Dwn Compression";
    lv2:portProperty lv2:toggled , lv2:integer ;
    lv2:default 1 ;
    lv2:minimum 0 ;
    lv2:maximum 1 ;
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 3;
    lv2:symbol "DOWN_THRES";
    lv2:name "Downwards Threshold";
    lv2:shortname "Dwn Threshold";
    lv2:default -20;
    lv2:minimum -50;
    lv2:maximum 0;
    units:unit   units:db
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 4;
    lv2:symbol "DOWN_KNEE";
    lv2:name "Downwards Knee";
    lv2:shortname "Dwn Knee";
    lv2:default 20;
    lv2:minimum 0;
    lv2:maximum 40;
    units:unit   units:db
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 5;
    lv2:symbol "DOWN_ATTACK";
    lv2:name "Downwards Attack";
    lv2:shortname "Dwn Attack";
    lv2:default 10;
    lv2:minimum 0.1;
    lv2:maximum 200;
    units:unit   units:ms
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 6;
    lv2:symbol "DOWN_RELEASE";
    lv2:name "Downwards Release";
    lv2:shortname "Dwn Release";
    lv2:default 100;
    lv2:minimum 1;
    lv2:maximum 5000;
    units:unit   units:ms
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 7;
    lv2:symbol "DOWN_RATIO";
    lv2:name "Downwards Ratio";
    lv2:shortname "Dwn Ratio";
    lv2:default 1;
    lv2:minimum 1;
    lv2:maximum 30;
    lv2:portProperty lv2:logarithmic;
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 8;
    lv2:symbol "DOWN_MAKEUP";
    lv2:name "Downwards Makeup Gain";
    lv2:shortname "Dwn MakeupGain";
    lv2:default 12;
    lv2:minimum -30;
    lv2:maximum 24;
    units:unit   units:db
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 9;
    lv2:symbol "UP_ON";
    lv2:name "Upwards Compression";
    lv2:shortname "Up Compression";
    lv2:portProperty lv2:toggled , lv2:integer ;
    lv2:default 1 ;
    lv2:minimum 0 ;
    lv2:maximum 1 ;
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 10;
    lv2:symbol "UP_THRES";
    lv2:name "Upwards Threshold";
    lv2:shortname "Up Threshold";
    lv2:default -40;
    lv2:minimum -70;
    lv2:maximum 0;
    units:unit   units:db
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 11;
    lv2:symbol "UP_KNEE";
    lv2:name "Upwards Knee";
    lv2:shortname "Up Knee";
    lv2:default 20;
    lv2:minimum 0;
    lv2:maximum 40;
    units:unit   units:db
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 12;
    lv2:symbol "UP_ATTACK";
    lv2:name "Upwards Attack";
    lv2:shortname "Up Attack";
    lv2:default 10;
    lv2:minimum 0.1;
    lv2:maximum 200;
    units:unit   units:ms
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 13;
    lv2:symbol "UP_RELEASE";
    lv2:name "Upwards Release";
    lv2:shortname "Up Release";
    lv2:default 100;
    lv2:minimum 1;
    lv2:maximum 5000;
    units:unit   units:ms
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 14;
    lv2:symbol "UP_RATIO";
    lv2:name "Upwards Ratio";
    lv2:shortname "Up Ratio";
    lv2:default 1;
    lv2:minimum 1;
    lv2:maximum 30;
    lv2:portProperty lv2:logarithmic;
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 15;
    lv2:symbol "UP_MAKEUP";
    lv2:name "Upwards Makeup Gain";
    lv2:shortname "Up MakeupGain";
    lv2:default 12;
    lv2:minimum -30;
    lv2:maximum 24;
    units:unit   units:db
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 16;
    lv2:symbol "LIMIT_THRESHOLD";
    lv2:name "Limiter Threshold";
    lv2:shortname "LimiterThreshold";
    lv2:default -3;
    lv2:minimum -50;
    lv2:maximum 0;
    units:unit   units:db
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 17;
    lv2:symbol "MASTER_VOL";
    lv2:name "Master Volume";
    lv2:shortname "MasterVol";
    lv2:default 0;
    lv2:minimum -30;
    lv2:maximum 30;
    units:unit   units:db
]
.
