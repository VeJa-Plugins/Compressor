@prefix lv2:  <http://lv2plug.in/ns/lv2core#>.
@prefix doap: <http://usefulinc.com/ns/doap#>.
@prefix epp: <http://lv2plug.in/ns/ext/port-props#>.
@prefix foaf: <http://xmlns.com/foaf/0.1/>.
@prefix mod: <http://moddevices.com/ns/modgui#>.
@prefix rdf:  <http://www.w3.org/1999/02/22-rdf-syntax-ns#>.
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#>.
@prefix pset: <http://lv2plug.in/ns/ext/presets#>.

<http://VeJaPlugins.com/plugins/Release/Compressor>
a lv2:Plugin, lv2:SimulatorPlugin;

doap:name "Compressor";

doap:developer [
    foaf:name "Jarno Verheese & Jan Janssen";
    foaf:homepage <>;
    foaf:mbox <mailto:jan@moddevices.com>;
    ];

doap:maintainer [
    foaf:name "VeJa plugins";
    foaf:homepage <http://moddevices.com>;
    foaf:mbox <mailto:jan@moddevices.com>;
    ];

    lv2:minorVersion 1;
    lv2:microVersion 0;

rdfs:comment """

Veja Compressor designed for mod devices

""";

lv2:port
[
    a lv2:AudioPort, lv2:InputPort;
    lv2:index 0;
    lv2:symbol "Input";
    lv2:name "Input";
    lv2:shortname "Input";
],
[
    a lv2:AudioPort, lv2:OutputPort;
    lv2:index 1;
    lv2:symbol "Output";
    lv2:name "Output";
    lv2:shortname "Output";
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 2;
    lv2:symbol "MakeupGain";
    lv2:name "Makeup Gain";
    lv2:shortname "MakeupGain";
    lv2:default 12;
    lv2:minimum -30;
    lv2:maximum 24;
    units:unit   units:db
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 3;
    lv2:symbol "CompressorThreshold";
    lv2:name "Compressor Threshold";
    lv2:shortname "CompressorThreshold";
    lv2:default -20;
    lv2:minimum -50;
    lv2:maximum 0;
    units:unit   units:db
],
[
    a lv2:InputPort, lv2:ControlPort;
    lv2:index 4;
    lv2:symbol "Knee";
    lv2:name "Knee";
    lv2:shortname "Knee";
    lv2:portProperty lv2:enumeration , lv2:integer ;
    lv2:default 0 ;
    lv2:minimum 0 ;
    lv2:maximum 1 ;
    lv2:scalePoint
    [
        rdfs:label "Soft" ;
        rdf:value 0 
    ] , [
        rdfs:label "Hard" ;
        rdf:value 1 
    ]
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 5;
    lv2:symbol "CompressorAttack";
    lv2:name "Compressor Attack";
    lv2:shortname "ComressorAttack";
    lv2:default 10;
    lv2:minimum 0.1;
    lv2:maximum 200;
    units:unit   units:ms
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 6;
    lv2:symbol "CompressorRelease";
    lv2:name "Compressor Release";
    lv2:shortname "CompressorRelease";
    lv2:default 100;
    lv2:minimum 1;
    lv2:maximum 5000;
    units:unit   units:ms
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 7;
    lv2:symbol "Ratio";
    lv2:name "Ratio";
    lv2:shortname "Ratio";
    lv2:default 1;
    lv2:minimum 1;
    lv2:maximum 30;
    lv2:portProperty lv2:logarithmic;
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 8;
    lv2:symbol "LimiterThreshold";
    lv2:name "Limiter Threshold";
    lv2:shortname "LimiterThreshold";
    lv2:default -20;
    lv2:minimum -50;
    lv2:maximum 0;
    units:unit   units:db
],
[
    a lv2:ControlPort, lv2:InputPort;
    lv2:index 9;
    lv2:symbol "MasterVolume";
    lv2:name "MasterVolume";
    lv2:shortname "LimiterMasterVolume";
    lv2:default 0;
    lv2:minimum -30;
    lv2:maximum 30;
    units:unit   units:db
]
.
